@font-face
  font-family 'sell-icon'
  src url('../fonts/sell-icon.eot?6i5wdx')
  src url('../fonts/sell-icon.eot?6i5wdx#iefix') format('embedded-opentype'), url('../fonts/sell-icon.ttf?6i5wdx') format('truetype'), url('../fonts/sell-icon.woff?6i5wdx') format('woff'), url('../fonts/sell-icon.svg?6i5wdx#sell-icon') format('svg')
  font-weight normal
  font-style normal

[class^='iconfont-'], [class*=' iconfont-']
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family 'sell-icon' !important
  speak none
  font-style normal
  font-weight normal
  font-variant normal
  text-transform none
  line-height 1
  /* Better Font Rendering =========== */
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

.iconfont-add_circle:before
  content '\e900'

.iconfont-arrow_lift:before
  content '\e901'

.iconfont-check_circle:before
  content '\e902'

.iconfont-close:before
  content '\e903'

.iconfont-favorite:before
  content '\e904'

.iconfont-keyboard_arrow_right:before
  content '\e905'

.iconfont-remove_circle_outline:before
  content '\e906'

.iconfont-shopping_cart:before
  content '\e907'

.iconfont-thumb_down:before
  content '\e908'

.iconfont-thumb_up:before
